{
  "name": "manual-j-calculator",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000",
    "build": "vite build && cp instrument.server.mjs .output/server",
    "serve": "vite preview",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run --run",
    "test:component": "vitest run --run --config vitest.config.ts",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "post-cta-init": "npx create-db@latest",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "start": "node --import ./.output/server/instrument.server.mjs .output/server/index.mjs",
    "type-check": "tsc --noEmit",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:a11y": "playwright test --grep '@a11y'",
    "test:visual": "percy exec -- playwright test --grep '@visual'",
    "lighthouse": "lhci autorun",
    "analyze": "rollup-plugin-visualizer"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@manualj/calc-engine": "workspace:*",
    "@manualj/ui": "workspace:*",
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^3.0.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@sentry/tanstackstart-react": "^10.23.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/db": "^0.4.20",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/query-db-collection": "^0.3.0",
    "@tanstack/react-db": "^0.1.43",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.134.15",
    "@tanstack/react-router-devtools": "^1.134.15",
    "@tanstack/react-router-ssr-query": "^1.134.15",
    "@tanstack/react-start": "^1.134.15",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/router-plugin": "^1.134.15",
    "@tanstack/store": "^0.8.0",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "2.2.4",
    "@lhci/cli": "^0.15.1",
    "@percy/playwright": "^1.0.9",
    "@playwright/test": "^1.56.1",
    "@tanstack/devtools-event-client": "^0.3.4",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "dotenv-cli": "^11.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8",
    "web-vitals": "^5.1.0"
  },
  "prisma": {
    "seed": "ts-node seed.ts"
  }
}